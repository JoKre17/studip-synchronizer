<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<Pane maxHeight="130.0" nodeOrientation="LEFT_TO_RIGHT" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="heightLimitingStackPane">
         <children>
            <GridPane hgap="10.0" maxWidth="1.7976931348623157E308">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="300.0" prefWidth="300.0" />
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="1.7976931348623157E308" valignment="TOP" />
              </rowConstraints>
               <children>
                  <Label fx:id="eventTitleLabel" maxWidth="1.7976931348623157E308" prefHeight="50.0" text="Event Title" />
                  <Label fx:id="eventContentLabel" maxWidth="1.7976931348623157E308" prefHeight="50.0" text="Event Content" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </StackPane>
      <VBox fx:id="seeMoreVBox" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308">
         <children>
            <HBox fx:id="seeMoreHBox" alignment="BOTTOM_CENTER" maxWidth="1.7976931348623157E308" prefWidth="81.0" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="seeMoreButton" alignment="CENTER" maxHeight="30.0" maxWidth="1.7976931348623157E308" minHeight="30.0" mnemonicParsing="false" text="see more" visible="false" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
